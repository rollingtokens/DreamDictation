  "daily_expressions": {
    "id": "daily-expressions",
    "name": "Daily expressions",
    "words": [
      {
        "text": "That's fantastic!",
        "translations": {
          "zh-CN": "妙极了！"
        }
      },
      {
        "text": "That's easy!",
        "translations": {
          "zh-CN": "那容易！"
        }
      },
      {
        "text": "Look carefully!",
        "translations": {
          "zh-CN": "仔细看！"
        }
      },
      {
        "text": "I hope so.",
        "translations": {
          "zh-CN": "我希望如此。"
        }
      },
      {
        "text": "Who is it for?",
        "translations": {
          "zh-CN": "它是给谁的？"
        }
      },
      {
        "text": "That's good.",
        "translations": {
          "zh-CN": "那不错。"
        }
      },
      {
        "text": "Enjoy your swimming lessons!",
        "translations": {
          "zh-CN": "游泳课上玩得愉快！"
        }
      },
      {
        "text": "Of course.",
        "translations": {
          "zh-CN": "当然可以。"
        }
      },
      {
        "text": "Please go ahead.",
        "translations": {
          "zh-CN": "请说吧。"
        }
      },
      {
        "text": "I see.",
        "translations": {
          "zh-CN": "我知道了。"
        }
      },
      {
        "text": "It's the right size!",
        "translations": {
          "zh-CN": "它大小正合适！"
        }
      },
      {
        "text": "Let's see.",
        "translations": {
          "zh-CN": "我们等着瞧吧。"
        }
      },
      {
        "text": "Your turn.",
        "translations": {
          "zh-CN": "轮到你了。"
        }
      },
      {
        "text": "Trick or treat?",
        "translations": {
          "zh-CN": "请吃糖，还是想遭殃？"
        }
      },
      {
        "text": "Is that you?",
        "translations": {
          "zh-CN": "是你吗？"
        }
      }
    ]
  },
